{ "name"                : "tinker"
, "descrioption"        : "IoT device gateway tinkerforge, rule-based, restful forker"
, "author"              : "robo.drone@e.mail.de"
, "license"             : "Free"
, "version"             : "1.0.0"
, "dependencies"        : {}
, "bundleDependencies"  : []
, "scripts"             : { "start"       : "node server.js"
                          , "build:sfile" :  "echo '=> sfile babel' && babel server.js -o ./bserver.js --compact --minified --no-comments"
            }
, "main"                : "server.js"
, "engines"             : { "node"  : ">= 0.10.0" , "npm": ">= 1.0.0"}
, "config"              : { "HOST"  : "localhost"
                          , "PORT"  : 4223
                          , "RHOST" : "r"
                          , "RPORT" : 80
                          , "RIDX"  : "home"
                          , "LOC"   : "M"
                          , "HID"   : "a"
                          , "BID"   : "t"
                          , "AID"   : "Z"
                          , "MID"   : "E"
                          , "RID"   : "q"
                          , "DELAY" : 15000
                          , "DELTA" : 0.001
                          }
}
